   "source": [
    "from geopy.geocoders import Nominatim"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "McDonald, Trumbull County, Ohio, 44437, United States\n",
      "Latitude =  41.163669 \n",
      "\n",
      "Longitude =  -80.724245\n"
     ]
    }
   ],
   "source": [
    "getLoc = input(\"Where are you?\")\n",
    "\n",
    "geo = Nominatim(user_agent=\"location\")\n",
    "\n",
    "location = geo.geocode(getLoc)\n",
    "\n",
    "print(location.address)\n",
    "\n",
    "print(\"Latitude = \", location.latitude, \"\\n\")\n",
    "print(\"Longitude = \", location.longitude)"
   ]
  },
